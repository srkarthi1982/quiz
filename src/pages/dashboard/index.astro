---
import LayoutFull from '../../app/LayoutFull.astro';
import Administrator from './administrator.astro'
import User from './user.astro';
const { user } = Astro.locals;
---
<LayoutFull active="dashboard">
  {
    user.user_metadata.role_id === 1 ?
    <Administrator user={user} /> :
    <User user={user} /> 
  }
</LayoutFull>